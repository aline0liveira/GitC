<Window x:Class="CalculadoraWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CalculadoraWPF"
         xmlns:y="clr-namespace:CalculadoraWPF.Views"
        mc:Ignorable="d"
        Title="Calculadora" Height="450" Width="800">
    <Grid>


        <y:ucLogin x:Name="ucLoginForm" loginCorrect="UcLoginForm_loginCorrect"></y:ucLogin>
        <y:ucCalculator x:Name="ucCalculator" Visibility="Hidden"></y:ucCalculator>



    </Grid>
</Window>
